<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="Ribbon_Load">
    <ribbon>

        <tabs>
            <tab idMso="TabHome">
                <group id="grpWordArt" insertAfterMso="GroupParagraph" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage">
                    <gallery idMso="TextStylesGallery" size="large" />
                    <control idMso="TextFillColorPicker" size="normal" />
                    <control idMso="TextOutlineColorPicker" size="normal" />
                    <control idMso="TextEffectsMenu" size="normal" />
                    <dialogBoxLauncher>
                        <button idMso="WordArtFormatDialog" />
                    </dialogBoxLauncher>
                </group>
                <group id="grpArrange" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage">
                    <buttonGroup id="grpAlignLR">
                        <button id="btnAlignLeft" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignCenter" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignRight" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpAlignTB">
                        <button id="btnAlignTop" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignMiddle" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignBottom" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpDistribute">
                        <button id="btnDistributeHorizontal" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnDistribute_Click" />
                        <button id="btnDistributeVertical" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnDistribute_Click" />
                        <button idMso="GridSettings" showLabel="false" />
                    </buttonGroup>
                    <separator id="separator1" />
                    <buttonGroup id="grpScaleSize">
                        <button id="btnScaleSameWidth" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnScale_Click" />
                        <button id="btnScaleSameHeight" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnScale_Click" />
                        <button id="btnScaleSameSize" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnScale_Click" />
                        <button id="btnScalePosition" showLabel="false" getLabel="GetBtnScalePositionLabel" getScreentip="GetBtnScalePositionLabel" getImage="GetBtnScalePositionImage" onAction="BtnScalePosition_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpExtendSize">
                        <button id="btnExtendSameLeft" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameRight" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameTop" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameBottom" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpSnapObjects">
                        <button id="btnSnapLeft" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                        <button id="btnSnapRight" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                        <button id="btnSnapTop" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                        <button id="btnSnapBottom" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                    </buttonGroup>
                    <separator id="separator2" />
                    <buttonGroup id="grpMoveLayers">
                        <button id="btnMoveForward" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                        <button id="btnMoveFront" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                        <button id="btnMoveBackward" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                        <button id="btnMoveBack" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpRotate">
                        <button id="btnRotateRight90" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnRotate_Click" />
                        <button id="btnRotateLeft90" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnRotate_Click" />
                        <button id="btnFlipVertical" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnFlip_Click" />
                        <button id="btnFlipHorizontal" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnFlip_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpGroup">
                        <button id="btnGroup" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnGroup_Click" />
                        <button id="btnUngroup" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnGroup_Click" />
                        <button idMso="ObjectSizeAndPositionDialog" showLabel="false" imageMso="ShapeStylesGallery" />
                        <toggleButton idMso="SelectionPane" showLabel="false" />
                    </buttonGroup>
                    <dialogBoxLauncher>
                        <button id="btnArrangementLauncher" onAction="BtnArrangementLauncher_Click" />
                    </dialogBoxLauncher>
                </group>
            </tab>
        </tabs>

        <contextualTabs>
            <tabSet idMso="TabSetDrawingTools">
                <tab idMso="TabDrawingToolsFormat">
                    <group id="grpTextbox" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" insertAfterMso="GroupWordArtStyles">
                        <toggleButton id="btnAutofitOff" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="GetBtnAutofitPressed" onAction="BtnAutofit_Click" />
                        <toggleButton id="btnAutofitText" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="GetBtnAutofitPressed" onAction="BtnAutofit_Click" />
                        <toggleButton id="btnAutoResize" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="GetBtnAutofitPressed" onAction="BtnAutofit_Click" />
                        <toggleButton id="btnWrapText" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="GetBtnWrapTextPressed" onAction="BtnWrapText_Click" />
                        <separator id="separator3" />
                        <editBox id="edtMarginLeft" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="GetEdtMarginText" onChange="EdtMargin_TextChanged" />
                        <editBox id="edtMarginRight" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="GetEdtMarginText" onChange="EdtMargin_TextChanged" />
                        <button id="btnResetMarginHorizontal" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnResetMargin_Click" />
                        <separator id="separator4" />
                        <editBox id="edtMarginTop" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="GetEdtMarginText" onChange="EdtMargin_TextChanged" />
                        <editBox id="edtMarginBottom" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="GetEdtMarginText" onChange="EdtMargin_TextChanged" />
                        <button id="btnResetMarginVertical" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnResetMargin_Click" />
                        <dialogBoxLauncher>
                            <button idMso="WordArtFormatDialog" />
                        </dialogBoxLauncher>
                    </group>
                    <group id="grpShapeSizeAndPosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage">
                        <dynamicMenu id="mnuShapeArrangement" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getContent="GetMnuArrangementContent" />
                        <separator id="separator5" />
                        <toggleButton id="btnLockShapeAspectRatio" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="GetBtnLockAspectRatio" onAction="BtnLockAspectRatio_Click" />
                        <button id="btnShapeScalePosition" getLabel="GetBtnScalePositionLabel" getScreentip="GetBtnScalePositionLabel" getImage="GetBtnScalePositionImage" onAction="BtnScalePosition_Click" />
                        <buttonGroup id="grpCopyAndPasteShapeSize">
                            <button id="btnCopyShapeSize" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                            <button id="btnPasteShapeSize" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                        </buttonGroup>
                        <separator id="separator6" />
                        <editBox id="edtShapePositionX" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getText="GetEdtPositionText" getEnabled="GetEnabled" onChange="EdtPosition_TextChanged" />
                        <editBox id="edtShapePositionY" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getText="GetEdtPositionText" getEnabled="GetEnabled" onChange="EdtPosition_TextChanged" />
                        <buttonGroup id="grpCopyAndPasteShapePosition">
                            <button id="btnCopyShapePosition" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                            <button id="btnPasteShapePosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                        </buttonGroup>
                        <dialogBoxLauncher>
                            <button idMso="ObjectSizeAndPositionDialog" />
                        </dialogBoxLauncher>
                    </group>
                </tab>
            </tabSet>
            <tabSet idMso="TabSetPictureTools">
                <tab idMso="TabPictureToolsFormat">
                    <group id="grpReplacePicture" insertAfterMso="GroupPictureTools" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage">
                        <button id="btnReplaceWithClipboard" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnReplacePicture_Click" />
                        <button id="btnReplaceWithFile" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnReplacePicture_Click" />
                        <checkBox id="cbxReserveOriginalSize" getLabel="GetLabel" getScreentip="GetLabel" getPressed="GetCbxReserveOriginalSize" onAction="CbxReserveOriginalSize_Click" />
                        <checkBox id="cbxReplaceToMiddle" getLabel="GetLabel" getScreentip="GetLabel" getPressed="GetCbxReplaceToMiddlePressed" onAction="CbxReplaceToMiddle_Click" />
                    </group>
                    <group id="grpPictureSizeAndPosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage">
                        <dynamicMenu id="mnuPictureArrangement" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getContent="GetMnuArrangementContent" />
                        <separator id="separator7" />
                        <button id="btnResetPictureSize" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnResetPictureSize_Click" />
                        <toggleButton id="btnLockPictureAspectRatio" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="GetBtnLockAspectRatio" onAction="BtnLockAspectRatio_Click" />
                        <button id="btnPictureScalePosition" getLabel="GetBtnScalePositionLabel" getScreentip="GetBtnScalePositionLabel" getImage="GetBtnScalePositionImage" onAction="BtnScalePosition_Click" />
                        <buttonGroup id="grpCopyAndPastePictureSize">
                            <button id="btnCopyPictureSize" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                            <button id="btnPastePictureSize" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                        </buttonGroup>
                        <separator id="separator8" />
                        <editBox id="edtPicturePositionX" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getText="GetEdtPositionText" getEnabled="GetEnabled" onChange="EdtPosition_TextChanged" />
                        <editBox id="edtPicturePositionY" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getText="GetEdtPositionText" getEnabled="GetEnabled" onChange="EdtPosition_TextChanged" />
                        <buttonGroup id="grpCopyAndPastePicturePosition">
                            <button id="btnCopyPicturePosition" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                            <button id="btnPastePicturePosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                        </buttonGroup>
                        <dialogBoxLauncher>
                            <button idMso="ObjectSizeAndPositionDialog" />
                        </dialogBoxLauncher>
                    </group>
                </tab>
            </tabSet>
        </contextualTabs>

    </ribbon>
</customUI>
