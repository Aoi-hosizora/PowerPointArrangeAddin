<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="Ribbon_Load">
    <ribbon>

        <tabs>
            <tab idMso="TabHome">
                <group id="grpWordArt" insertAfterMso="GroupParagraph" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getVisible="GetGroupVisible">
                    <control idMso="TextStylesGallery" size="large" keytip="AQ" />
                    <control idMso="TextFillColorPicker" keytip="AF" />
                    <control idMso="TextOutlineColorPicker" keytip="AU" />
                    <menu idMso="TextEffectsMenu" keytip="AE" />
                    <dialogBoxLauncher>
                        <button idMso="WordArtFormatDialog" keytip="AG" />
                    </dialogBoxLauncher>
                </group> <!-- end of grpWordArt -->

                <group id="grpArrange" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage">
                    <buttonGroup id="grpAlignLR">
                        <button id="btnAlignLeft" keytip="D1" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignCenter" keytip="D2" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignRight" keytip="D3" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpAlignTB">
                        <button id="btnAlignTop" keytip="D4" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignMiddle" keytip="D5" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                        <button id="btnAlignBottom" keytip="D6" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnAlign_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpDistribute">
                        <button id="btnDistributeHorizontal" keytip="D7" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnDistribute_Click" />
                        <button id="btnDistributeVertical" keytip="D8" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnDistribute_Click" />
                        <button idMso="GridSettings" keytip="DG" showLabel="false" />
                    </buttonGroup>
                    <separator id="separator1" />
                    <buttonGroup id="grpScaleSize">
                        <button id="btnScaleSameWidth" keytip="PW" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnScale_Click" />
                        <button id="btnScaleSameHeight" keytip="PH" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnScale_Click" />
                        <button id="btnScaleSameSize" keytip="PS" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnScale_Click" />
                        <button id="btnScaleAnchor" keytip="PA" showLabel="false" getLabel="BtnScaleAnchor_GetLabel" getScreentip="BtnScaleAnchor_GetLabel" getImage="BtnScaleAnchor_GetImage" onAction="BtnScaleAnchor_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpExtendSize">
                        <button id="btnExtendSameLeft" keytip="PL" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameRight" keytip="PR" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameTop" keytip="PT" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameBottom" keytip="PB" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnExtend_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpSnapObjects">
                        <button id="btnSnapLeft" keytip="PE" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                        <button id="btnSnapRight" keytip="PI" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                        <button id="btnSnapTop" keytip="PO" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                        <button id="btnSnapBottom" keytip="PM" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnSnap_Click" />
                    </buttonGroup>
                    <separator id="separator2" />
                    <buttonGroup id="grpMoveLayers">
                        <button id="btnMoveForward" keytip="HF" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                        <button id="btnMoveFront" keytip="HO" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                        <button id="btnMoveBackward" keytip="HB" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                        <button id="btnMoveBack" keytip="HK" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnMove_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpRotate">
                        <button id="btnRotateRight90" keytip="HR" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnRotate_Click" />
                        <button id="btnRotateLeft90" keytip="HL" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnRotate_Click" />
                        <button id="btnFlipVertical" keytip="HV" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnFlip_Click" />
                        <button id="btnFlipHorizontal" keytip="HH" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnFlip_Click" />
                    </buttonGroup>
                    <buttonGroup id="grpGroupObjects">
                        <button id="btnGroup" keytip="HG" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnGroup_Click" />
                        <button id="btnUngroup" keytip="HU" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnGroup_Click" />
                        <button idMso="ObjectSizeAndPositionDialog" keytip="HD" showLabel="false" imageMso="ShapeStylesGallery" />
                        <toggleButton idMso="SelectionPane" keytip="HP" showLabel="false" />
                    </buttonGroup>
                    <separator id="separator3" />
                    <dynamicMenu id="mnuArrangement" keytip="B" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getContent="MnuArrangement_GetContent" />
                    <dialogBoxLauncher>
                        <button id="btnAddInSetting" keytip="HT" getLabel="GetLabel" getScreentip="GetLabel" onAction="BtnAddInSetting_Click" />
                    </dialogBoxLauncher>
                </group> <!-- end of grpArrange -->
            </tab> <!-- end of TabHome -->
        </tabs>

        <contextualTabs>
            <tabSet idMso="TabSetDrawingTools">
                <tab idMso="TabDrawingToolsFormat">
                    <group id="grpTextbox" insertAfterMso="GroupWordArtStyles" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getVisible="GetGroupVisible">
                        <toggleButton keytip="TF" id="btnAutofitOff" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="BtnAutofit_GetPressed" onAction="BtnAutofit_Click" />
                        <toggleButton keytip="TS" id="btnAutoShrinkText" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="BtnAutofit_GetPressed" onAction="BtnAutofit_Click" />
                        <toggleButton keytip="TR" id="btnAutoResizeShape" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="BtnAutofit_GetPressed" onAction="BtnAutofit_Click" />
                        <toggleButton keytip="TW" id="btnWrapText" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="BtnWrapText_GetPressed" onAction="BtnWrapText_Click" />
                        <separator id="separator4" />
                        <editBox keytip="ML" id="edtMarginLeft" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <editBox keytip="MR" id="edtMarginRight" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <button keytip="MH" id="btnResetHorizontalMargin" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnResetMargin_Click" />
                        <separator id="separator5" />
                        <editBox keytip="MT" id="edtMarginTop" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <editBox keytip="MB" id="edtMarginBottom" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <button keytip="MV" id="btnResetVerticalMargin" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnResetMargin_Click" />
                        <dialogBoxLauncher>
                            <button keytip="TG" idMso="WordArtFormatDialog" />
                        </dialogBoxLauncher>
                    </group> <!-- end of grpTextbox -->

                    <group id="grpShapeSizeAndPosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getVisible="GetGroupVisible">
                        <dynamicMenu keytip="B" id="mnuShapeArrangement" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getContent="MnuArrangement_GetContent" />
                        <separator id="separator6" />
                        <toggleButton keytip="L" id="btnLockShapeAspectRatio" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="BtnLockAspectRatio_GetPressed" onAction="BtnLockAspectRatio_Click" />
                        <button keytip="PA" id="btnShapeScaleAnchor" getLabel="BtnScaleAnchor_GetLabel" getScreentip="BtnScaleAnchor_GetLabel" getImage="BtnScaleAnchor_GetImage" onAction="BtnScaleAnchor_Click" />
                        <buttonGroup id="grpCopyAndPasteShapeSize">
                            <button keytip="SC" id="btnCopyShapeSize" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                            <button keytip="SP" id="btnPasteShapeSize" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                        </buttonGroup>
                        <separator id="separator7" />
                        <editBox keytip="PX" id="edtShapePositionX" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="EdtPosition_GetText" onChange="EdtPosition_TextChanged" />
                        <editBox keytip="PY" id="edtShapePositionY" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getEnabled="GetEnabled" getText="EdtPosition_GetText" onChange="EdtPosition_TextChanged" />
                        <buttonGroup id="grpCopyAndPasteShapePosition">
                            <button keytip="PC" id="btnCopyShapePosition" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                            <button keytip="PP" id="btnPasteShapePosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                        </buttonGroup>
                        <dialogBoxLauncher>
                            <button keytip="SN" idMso="ObjectSizeAndPositionDialog" />
                        </dialogBoxLauncher>
                    </group> <!-- end of grpShapeSizeAndPosition -->
                </tab> <!-- end of TabDrawingToolsFormat -->
            </tabSet>

            <tabSet idMso="TabSetPictureTools">
                <tab idMso="TabPictureToolsFormat">
                    <group id="grpReplacePicture" insertAfterMso="GroupPictureTools" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getVisible="GetGroupVisible">
                        <button keytip="TC" id="btnReplaceWithClipboard" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnReplacePicture_Click" />
                        <button keytip="TF" id="btnReplaceWithFile" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnReplacePicture_Click" />
                        <checkBox keytip="TR" id="chkReserveOriginalSize" getLabel="GetLabel" getScreentip="GetLabel" getPressed="ChkReserveOriginalSize_GetPressed" onAction="ChkReserveOriginalSize_Click" />
                        <checkBox keytip="TM" id="chkReplaceToMiddle" getLabel="GetLabel" getScreentip="GetLabel" getPressed="ChkReplaceToMiddle_GetPressed" onAction="ChkReplaceToMiddle_Click" />
                    </group> <!-- end of grpReplacePicture -->

                    <group id="grpPictureSizeAndPosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getVisible="GetGroupVisible">
                        <dynamicMenu keytip="B" id="mnuPictureArrangement" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getContent="MnuArrangement_GetContent" />
                        <separator id="separator8" />
                        <button keytip="SR" id="btnResetPictureSize" size="large" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnResetPictureSize_Click" />
                        <toggleButton keytip="L" id="btnLockPictureAspectRatio" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" getPressed="BtnLockAspectRatio_GetPressed" onAction="BtnLockAspectRatio_Click" />
                        <button keytip="PA" id="btnPictureScaleAnchor" getLabel="BtnScaleAnchor_GetLabel" getScreentip="BtnScaleAnchor_GetLabel" getImage="BtnScaleAnchor_GetImage" onAction="BtnScaleAnchor_Click" />
                        <buttonGroup id="grpCopyAndPastePictureSize">
                            <button keytip="SC" id="btnCopyPictureSize" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                            <button keytip="SP" id="btnPastePictureSize" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPasteSize_Click" />
                        </buttonGroup>
                        <separator id="separator9" />
                        <editBox keytip="PX" id="edtPicturePositionX" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getText="EdtPosition_GetText" getEnabled="GetEnabled" onChange="EdtPosition_TextChanged" />
                        <editBox keytip="PY" id="edtPicturePositionY" showImage="false" sizeString="wwwwwww" getLabel="GetLabel" getScreentip="GetLabel" getText="EdtPosition_GetText" getEnabled="GetEnabled" onChange="EdtPosition_TextChanged" />
