                            <button keytip="PC" id="btnCopyPicturePosition" showLabel="false" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                            <button keytip="PP" id="btnPastePicturePosition" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getEnabled="GetEnabled" onAction="BtnCopyAndPastePosition_Click" />
                        </buttonGroup>
                        <dialogBoxLauncher>
                            <button keytip="SN" idMso="ObjectSizeAndPositionDialog" />
                        </dialogBoxLauncher>
                    </group> <!-- end of grpPictureSizeAndPosition -->
                </tab> <!-- end of TabPictureToolsFormat -->
            </tabSet>
        </contextualTabs>

    </ribbon>
</customUI>
