<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="Ribbon_Load">
    <__templates>
        <__template name="L" getLabel="GetLabel" />
        <__template name="S" getScreentip="GetLabel" />
        <__template name="K" getKeytip="GetKeytip" />
        <__template name="E" getEnabled="GetEnabled" />
        <__template name="I" getImage="GetImage" />
        <__template name="Z" sizeString="wwwwwww" />
        <__template name="CV" getVisible="GetControlVisible" />
        <__template name="GV" getVisible="GetGroupVisible" />
        <__template name="GROUP" getLabel="GetLabel" getScreentip="GetLabel" getImage="GetImage" getVisible="GetGroupVisible" />
        <__template name="LSKI_Anchor" getLabel="BtnScaleAnchor_GetLabel" getScreentip="BtnScaleAnchor_GetLabel" getKeytip="GetKeytip" getImage="BtnScaleAnchor_GetImage" />
    </__templates>

    <ribbon>
        <tabs>
            <tab idMso="TabHome">
                <group id="grpWordArt" insertAfterMso="GroupParagraph" __template="GROUP">
                    <control idMso="TextStylesGallery" size="large" __template="K" />
                    <control idMso="TextFillColorPicker" __template="K" />
                    <control idMso="TextOutlineColorPicker" __template="K" />
                    <menu idMso="TextEffectsMenu" __template="K" />
                    <dialogBoxLauncher>
                        <button idMso="WordArtFormatDialog" __template="K" />
                    </dialogBoxLauncher>
                </group> <!-- end of grpWordArt -->

                <group id="grpArrange" insertAfterMso="GroupEditing" __template="GROUP">
                    <buttonGroup id="bgpAlignLR">
                        <button id="btnAlignLeft" showLabel="false" __template="L,S,K,E,I" onAction="BtnAlign_Click" />
                        <button id="btnAlignCenter" showLabel="false" __template="L,S,K,E,I" onAction="BtnAlign_Click" />
                        <button id="btnAlignRight" showLabel="false" __template="L,S,K,E,I" onAction="BtnAlign_Click" />
                    </buttonGroup>
                    <buttonGroup id="bgpAlignTB">
                        <button id="btnAlignTop" showLabel="false" __template="L,S,K,E,I" onAction="BtnAlign_Click" />
                        <button id="btnAlignMiddle" showLabel="false" __template="L,S,K,E,I" onAction="BtnAlign_Click" />
                        <button id="btnAlignBottom" showLabel="false" __template="L,S,K,E,I" onAction="BtnAlign_Click" />
                    </buttonGroup>
                    <buttonGroup id="bgpDistribute">
                        <button id="btnDistributeHorizontal" showLabel="false" __template="L,S,K,E,I" onAction="BtnDistribute_Click" />
                        <button id="btnDistributeVertical" showLabel="false" __template="L,S,K,E,I" onAction="BtnDistribute_Click" />
                        <button idMso="GridSettings" showLabel="false" __template="K" />
                    </buttonGroup>
                    <separator id="grpArrange_separator1" />
                    <buttonGroup id="bgpScaleSize">
                        <button id="btnScaleSameWidth" showLabel="false" __template="L,S,K,E,I" onAction="BtnScale_Click" />
                        <button id="btnScaleSameHeight" showLabel="false" __template="L,S,K,E,I" onAction="BtnScale_Click" />
                        <button id="btnScaleSameSize" showLabel="false" __template="L,S,K,E,I" onAction="BtnScale_Click" />
                        <button id="btnScaleAnchor" showLabel="false" __template="LSKI_Anchor" onAction="BtnScaleAnchor_Click" />
                    </buttonGroup>
                    <buttonGroup id="bgpExtendSize">
                        <button id="btnExtendSameLeft" showLabel="false" __template="L,S,K,E,I" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameRight" showLabel="false" __template="L,S,K,E,I" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameTop" showLabel="false" __template="L,S,K,E,I" onAction="BtnExtend_Click" />
                        <button id="btnExtendSameBottom" showLabel="false" __template="L,S,K,E,I" onAction="BtnExtend_Click" />
                    </buttonGroup>
                    <buttonGroup id="bgpSnapObjects">
                        <button id="btnSnapLeft" showLabel="false" __template="L,S,K,E,I" onAction="BtnSnap_Click" />
                        <button id="btnSnapRight" showLabel="false" __template="L,S,K,E,I" onAction="BtnSnap_Click" />
                        <button id="btnSnapTop" showLabel="false" __template="L,S,K,E,I" onAction="BtnSnap_Click" />
                        <button id="btnSnapBottom" showLabel="false" __template="L,S,K,E,I" onAction="BtnSnap_Click" />
                    </buttonGroup>
                    <separator id="grpArrange_separator2" __template="CV" />
                    <buttonGroup id="bgpMoveLayers" __template="CV">
                        <button id="btnMoveForward" showLabel="false" __template="L,S,K,E,I" onAction="BtnMove_Click" />
                        <button id="btnMoveFront" showLabel="false" __template="L,S,K,E,I" onAction="BtnMove_Click" />
                        <button id="btnMoveBackward" showLabel="false" __template="L,S,K,E,I" onAction="BtnMove_Click" />
                        <button id="btnMoveBack" showLabel="false" __template="L,S,K,E,I" onAction="BtnMove_Click" />
                    </buttonGroup>
                    <buttonGroup id="bgpRotate" __template="CV">
                        <button id="btnRotateRight90" showLabel="false" __template="L,S,K,E,I" onAction="BtnRotate_Click" />
                        <button id="btnRotateLeft90" showLabel="false" __template="L,S,K,E,I" onAction="BtnRotate_Click" />
                        <button id="btnFlipVertical" showLabel="false" __template="L,S,K,E,I" onAction="BtnFlip_Click" />
                        <button id="btnFlipHorizontal" showLabel="false" __template="L,S,K,E,I" onAction="BtnFlip_Click" />
                    </buttonGroup>
                    <buttonGroup id="bgpGroupObjects" __template="CV">
                        <button id="btnGroup" showLabel="false" __template="L,S,K,E,I" onAction="BtnGroup_Click" />
                        <button id="btnUngroup" showLabel="false" __template="L,S,K,E,I" onAction="BtnGroup_Click" />
                        <button idMso="ObjectSizeAndPositionDialog" showLabel="false" __template="K" imageMso="ShapeStylesGallery" />
                        <toggleButton idMso="SelectionPane" showLabel="false" __template="K" />
                    </buttonGroup>
                    <separator id="grpArrange_separator3" __template="CV" />
                    <dynamicMenu id="mnuArrangement" size="large" __template="L,S,K,E,I,CV" getContent="GetMenuContent" />
                    <dialogBoxLauncher>
                        <button id="btnAddInSetting" __template="L,S,K" onAction="BtnAddInSetting_Click" />
                    </dialogBoxLauncher>
                </group> <!-- end of grpArrange -->
            </tab> <!-- end of TabHome -->
        </tabs>

        <contextualTabs>
            <tabSet idMso="TabSetDrawingTools">
                <tab idMso="TabDrawingToolsFormat">
                    <group id="grpTextbox" insertAfterMso="GroupWordArtStyles" __template="GROUP">
                        <toggleButton id="btnAutofitOff" __template="L,S,K,E,I" getPressed="BtnAutofit_GetPressed" onAction="BtnAutofit_Click" />
                        <toggleButton id="btnAutoShrinkText" __template="L,S,K,E,I" getPressed="BtnAutofit_GetPressed" onAction="BtnAutofit_Click" />
                        <toggleButton id="btnAutoResizeShape" __template="L,S,K,E,I" getPressed="BtnAutofit_GetPressed" onAction="BtnAutofit_Click" />
                        <toggleButton id="btnWrapText" size="large" __template="L,S,K,E,I" getPressed="BtnWrapText_GetPressed" onAction="BtnWrapText_Click" />
                        <separator id="grpTextbox_separator1" />
                        <editBox id="edtMarginLeft" showImage="false" __template="L,S,K,E,Z" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <editBox id="edtMarginRight" showImage="false" __template="L,S,K,E,Z" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <button id="btnResetHorizontalMargin" __template="L,S,K,E,I" onAction="BtnResetMargin_Click" />
                        <separator id="grpTextbox_separator2" />
                        <editBox id="edtMarginTop" showImage="false" __template="L,S,K,E,Z" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <editBox id="edtMarginBottom" showImage="false" __template="L,S,K,E,Z" getText="EdtMargin_GetText" onChange="EdtMargin_TextChanged" />
                        <button id="btnResetVerticalMargin" __template="L,S,K,E,I" onAction="BtnResetMargin_Click" />
                        <dialogBoxLauncher>
                            <button idMso="WordArtFormatDialog" __template="K" />
                        </dialogBoxLauncher>
                    </group> <!-- end of grpTextbox -->

                    <group id="grpShapeSizeAndPosition" insertAfterMso="GroupSize" __template="GROUP" __subtree_as_template="SIZEPOS_SHAPE">
                        <dynamicMenu id="mnuShapeArrangement" size="large" __template="L,S,K,E,I" getContent="GetMenuContent" />
                        <separator id="grpShapeSizeAndPosition_separator1" />
                        <toggleButton id="btnLockShapeAspectRatio" __template="L,S,K,E,I" getPressed="BtnLockAspectRatio_GetPressed" onAction="BtnLockAspectRatio_Click" />
                        <button id="btnShapeScaleAnchor" __template="LSKI_Anchor" onAction="BtnScaleAnchor_Click" />
                        <buttonGroup id="bgpCopyAndPasteShapeSize">
                            <button id="btnCopyShapeSize" showLabel="false" __template="L,S,K,E,I" onAction="BtnCopyAndPasteSize_Click" />
                            <button id="btnPasteShapeSize" __template="L,S,K,E,I" onAction="BtnCopyAndPasteSize_Click" />
                        </buttonGroup>
                        <separator id="grpShapeSizeAndPosition_separator2" />
                        <editBox id="edtShapePositionX" showImage="false" __template="L,S,K,E,Z" getText="EdtPosition_GetText" onChange="EdtPosition_TextChanged" />
                        <editBox id="edtShapePositionY" showImage="false" __template="L,S,K,E,Z" getText="EdtPosition_GetText" onChange="EdtPosition_TextChanged" />
                        <buttonGroup id="bgpCopyAndPasteShapePosition">
                            <button id="btnCopyShapePosition" showLabel="false" __template="L,S,K,E,I" onAction="BtnCopyAndPastePosition_Click" />
                            <button id="btnPasteShapePosition" __template="L,S,K,E,I" onAction="BtnCopyAndPastePosition_Click" />
                        </buttonGroup>
                        <dialogBoxLauncher>
                            <button idMso="ObjectSizeAndPositionDialog" __template="K" />
                        </dialogBoxLauncher>
                    </group> <!-- end of grpShapeSizeAndPosition -->
                </tab> <!-- end of TabDrawingToolsFormat -->
            </tabSet> <!-- end of TabSetDrawingTools -->

            <tabSet idMso="TabSetPictureTools">
                <tab idMso="TabPictureToolsFormat">
                    <group id="grpReplacePicture" insertAfterMso="GroupPictureTools" __template="GROUP">
                        <button id="btnReplaceWithClipboard" size="large" __template="L,S,K,E,I" onAction="BtnReplacePicture_Click" />
                        <button id="btnReplaceWithFile" __template="L,S,K,E,I" onAction="BtnReplacePicture_Click" />
                        <checkBox id="chkReserveOriginalSize" __template="L,S,K,E" getPressed="ChkReserveOriginalSize_GetPressed" onAction="ChkReserveOriginalSize_Click" />
                        <checkBox id="chkReplaceToMiddle" __template="L,S,K,E" getPressed="ChkReplaceToMiddle_GetPressed" onAction="ChkReplaceToMiddle_Click" />
                    </group> <!-- end of grpReplacePicture -->

                    <group id="grpPictureSizeAndPosition" insertAfterMso="GroupPictureSize" __template="GROUP" __subtree_as_template="SIZEPOS_PICTURE">
                        <dynamicMenu id="mnuPictureArrangement" size="large" __template="L,S,K,E,I" getContent="GetMenuContent" />
                        <separator id="grpPictureSizeAndPosition_separator1" />
                        <button id="btnResetPictureSize" size="large" __template="L,S,K,E,I" onAction="BtnResetMediaSize_Click" />
                        <toggleButton id="btnLockPictureAspectRatio" __template="L,S,K,E,I" getPressed="BtnLockAspectRatio_GetPressed" onAction="BtnLockAspectRatio_Click" />
                        <button id="btnPictureScaleAnchor" __template="LSKI_Anchor" onAction="BtnScaleAnchor_Click" />
                        <buttonGroup id="bgpCopyAndPastePictureSize">
                            <button id="btnCopyPictureSize" showLabel="false" __template="L,S,K,E,I" onAction="BtnCopyAndPasteSize_Click" />
                            <button id="btnPastePictureSize" __template="L,S,K,E,I" onAction="BtnCopyAndPasteSize_Click" />
                        </buttonGroup>
                        <separator id="grpPictureSizeAndPosition_separator2" />
                        <editBox id="edtPicturePositionX" showImage="false" __template="L,S,K,E,Z" getText="EdtPosition_GetText" onChange="EdtPosition_TextChanged" />
                        <editBox id="edtPicturePositionY" showImage="false" __template="L,S,K,E,Z" getText="EdtPosition_GetText" onChange="EdtPosition_TextChanged" />
                        <buttonGroup id="bgpCopyAndPastePicturePosition">
                            <button id="btnCopyPicturePosition" showLabel="false" __template="L,S,K,E,I" onAction="BtnCopyAndPastePosition_Click" />
                            <button id="btnPastePicturePosition" __template="L,S,K,E,I" onAction="BtnCopyAndPastePosition_Click" />
                        </buttonGroup>
                        <dialogBoxLauncher>
                            <button idMso="ObjectSizeAndPositionDialog" __template="K" />
                        </dialogBoxLauncher>
                    </group> <!-- end of grpPictureSizeAndPosition -->
                </tab> <!-- end of TabPictureToolsFormat -->
            </tabSet> <!-- end of TabSetPictureTools -->

            <tabSet idMso="TabSetVideoTools">
                <tab idMso="TabVideoToolsDesign">
                    <group id="grpVideoSizeAndPosition" insertAfterMso="GroupVideoSize" __template="GROUP">
                        <__apply_subtree_template use_template="SIZEPOS_PICTURE" replace_field="id" replace_from="Picture" replace_to="Video" />
                    </group>
                </tab>
            </tabSet>

            <tabSet idMso="TabSetAudioTools">
                <tab idMso="TabAudioToolsFormat">
                    <group id="grpAudioSizeAndPosition" insertAfterMso="GroupPictureSize" __template="GROUP">
                        <__apply_subtree_template use_template="SIZEPOS_PICTURE" replace_field="id" replace_from="Picture" replace_to="Audio" />
                    </group>
                </tab>
            </tabSet>

            <tabSet idMso="TabSetTableTools">
                <tab idMso="TabTableToolsLayout">
                    <group id="grpTableSizeAndPosition" insertAfterMso="GroupArrange" __template="GROUP">
                        <__apply_subtree_template use_template="SIZEPOS_SHAPE" replace_field="id" replace_from="Shape" replace_to="Table" />
                    </group>
                </tab>
            </tabSet>

            <tabSet idMso="TabSetChartTools">
                <tab idMso="TabChartToolsFormat">
                    <group id="grpChartSizeAndPosition" insertAfterMso="GroupSize" __template="GROUP">
                        <__apply_subtree_template use_template="SIZEPOS_SHAPE" replace_field="id" replace_from="Shape" replace_to="Chart" />
                    </group>
                </tab>
            </tabSet>

            <tabSet idMso="TabSetSmartArtTools">
                <tab idMso="TabSmartArtToolsFormat">
                    <group id="grpSmartartSizeAndPosition" insertAfterMso="GroupSmartArtSize" __template="GROUP">
                        <__apply_subtree_template use_template="SIZEPOS_SHAPE" replace_field="id" replace_from="Shape" replace_to="Smartart" />
                    </group>
                </tab>
            </tabSet>

            <!-- Remaining: CDAudio, Movie, Sound -->
        </contextualTabs>
    </ribbon>
</customUI>
